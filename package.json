{
  "name": "tracker-server",
  "version": "1.0.0",
  "description": "前端监控后台",
  "private": true,
  "dependencies": {
    "@jyb/egg-view-jyb": "^1.0.3",
    "egg": "^1.7.0",
    "egg-scripts": "^1.0.0",
    "egg-sequelize": "^3.1.5",
    "egg-validate": "^1.0.0",
    "egg-view-nunjucks": "^2.1.3",
    "moment": "^2.17.1",
    "moment-range": "^3.0.3",
    "mysql2": "^1.4.2",
    "nodemailer": "^4.4.2",
    "source-map": "^0.6.0"
  },
  "devDependencies": {
    "@jyb/eslint-config-jyb": "^1.0.1",
    "autod": "^2.9.0",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.1.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.9.0",
    "eslint": "^4.3.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "pm2": "pm2 start index.js --name \"tracker\"",
    "restart": "pm2 restart tracker",
    "start": "NODE_ENV=production node index.js",
    "start:scripts": "egg-scripts start --daemon --port=7002",
    "stop:scripts": "egg-scripts stop",
    "dev": "egg-bin dev --port=7002",
    "watch": "jfet build -w",
    "build": "NODE_ENV=production jfet build",
    "release": "sh release.sh",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "6, 8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "jfetOptions": {
    "configFilePath": "web"
  },
  "author": "canye.wu@jyblife.com",
  "license": "MIT"
}
